  "scripts": {
    "clean": "rimraf _site",
    "sass": "sass --no-source-map assets/sass:_site/css",
    "postcss": "postcss _site/css/*.css --no-map --replace",
    "build:sass": "npm-run-all --sequential sass postcss",
    "watch:sass": "sass --source-map-urls=absolute --watch assets/sass:_site/css",
    "build:js": "rollup -c",
    "watch:js": "rollup -c --watch",
    "build:eleventy": "NODE_OPTIONS='-r esm' eleventy",
    "watch:eleventy": "NODE_OPTIONS='-r esm' eleventy --serve",
    "serviceworker:inject": "workbox generateSW workbox.config.js",
    "start": " NODE_ENV=development npm-run-all --sequential clean build:js --parallel watch:*",
    "start0": " NODE_ENV=development npm-run-all --sequential clean --parallel build:js build:sass --sequential build:eleventy ",
    "startsup": " NODE_ENV=production npm-run-all  --sequential clean --parallel build:js build:sass serviceworker:inject --parallel watch:*",
    "start:debug": "DEBUG=Eleventy* npm start",
    "build": " NODE_ENV=production npm-run-all --sequential clean --parallel build:js build:sass --sequential build:eleventy serviceworker:inject",
    "build:debug": "DEBUG=Eleventy* npm run build",
    "build:benchmark": "DEBUG=Eleventy:Benchmark* npm run build",
    "build:netlify": " NODE_ENV=production npm-run-all --sequential clean --parallel build:js build:sass --sequential build:eleventy serviceworker:inject",
    "webmention": "node ./_scripts/update-webmention.js",
    "likes": "node ./_scripts/twitterLikes.js",
    "rglikes": "node ./_scripts/researchgatesLikes.js",
    "tweets": "node ./_scripts/send-tweets.js",
    "sprite": "node ./_scripts/generate-svg-sprite.js",
    "plum": "node ./_scripts/plum.js",
    "people": "node ./_scripts/updateAuthors.js",
    "bib": "(cd BIB; ./updatebib.sh)",
    "update": "npm-run-all start0 bib sprite webmention",
    "updateall": "npm-run-all start0 bib sprite webmention plum start0  rglikes likes  start0",
    "lint:sass": "stylelint 'assets/**/*.scss'",
    "fix:sass": "stylelint --fix 'assets/**/*.scss'"
  },
