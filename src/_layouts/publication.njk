<i>{{article.journal | title-}}</i>.{%endif%}
{% if not article.mainlink==""%}<a href="{{article.mainlink}}">Publisher</a> {%endif%}
{% if not article.pdflink==""%}
<a href="{{article.pdflink}}">Eprint </a>
{%endif%}

 </li >
