---
layout: content
eleventyComputed:
    orderDate: "{{-page.date | attributeDate-}}"
    title: "SAMA documentation - {{title}}"
---



<article
    class="page wrap h-entry"
    lang="{{ lang }}">




    {#<h1 class="page__title p-name">{{ title | orphans | safe }}</h1>#}



  <div class=" page__body e-content indent wrap0">
  <div class="centerList">
    <h2>Navigation:</h2> 
    
    {{ collections.all | eleventyNavigation | eleventyNavigationToHtml(
    {
    activeListItemClass: "selected-A", 
    activeKey: eleventyNavigation.key
    }) | safe }}
    </div>
    
     <div class="legibilityWidth wrap0all">
  {# <h2>Table of content:</h2>
   {{ content | toc(tags=['h2', 'h3'], wrapperClass='autotoc ') | safe }}
  #}
      {{ content | safe }}
      </div>
  
  <div class="centerList">
      <h2>Navigation:</h2> 

  
    {{ collections.all | eleventyNavigation | eleventyNavigationToHtml(
    {
    activeListItemClass: "selected-A", 
    activeKey: eleventyNavigation.key
    }) | safe }}
    </div>
  
  </div>



  

  </article>

