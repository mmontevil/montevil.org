{% if build.env == 'production' %}
  {% set css0 %}
  {%- include_raw config.dir.dist + "/css/" + hashes_css[cssFilename] -%}
  {% endset %}
 {%if nopurge%}
      <style>{{-  css0 | safe -}}</style>
 {%else%}
        <style>{%- purgeCss content, css0 -%}</style>
  {%endif%}
{% else %}
  <link rel="stylesheet" href="/css/{{ cssFilename }}" />
{% endif %}
