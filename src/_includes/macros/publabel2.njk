  <li class="publink2 meta__item publinkCommentary2">{% icon "book", "book" %} Commentary</li>
  {%- elif item.typepub === "data" -%}
  <li class="publink2 meta__item publinkData2 ">{% icon "book", "book" %} Data</li>
 {%- elif item.typepub === "book" -%}
 <li class="publink2 meta__item publinkBook2 ">{% icon "book", "book" %} Book</li>
 {%- endif-%}
 {%- if item.peerReview-%}
