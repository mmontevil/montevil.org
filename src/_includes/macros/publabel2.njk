 
{% macro publabel2(item) %}
 <li class=" meta__item ">{% icon "message", "message" %}
 <span class="  publinklang2">{{-item.lang-}}</span></li>
 {%- if item.typepub === "Article"-%}
 <li class="publink2  meta__item">{% icon "book", "book" %} Article</li>
 {%- elif item.typepub === "chapter" -%}
 <li class="publink2 meta__item publinkchap2 ">{% icon "book", "book" %} Chapter</li>
