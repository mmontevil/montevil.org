    and
    <!--l. 923-->
    <math display="inline" xmlns="http://www.w3.org/1998/Math/MathML"><mi>ν</mi> <mo class="MathClass-rel">&gt;</mo> <mn>0</mn></math> when
    <!--l. 924-->
    <math display="inline" xmlns="http://www.w3.org/1998/Math/MathML">
        <mrow>
            <mo class="MathClass-open">(</mo>
