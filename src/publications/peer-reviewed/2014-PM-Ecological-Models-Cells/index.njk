                                    </msub>
                                    <msub>
                                        <mrow><mi>q</mi></mrow><mrow><mn>1</mn><mn>2</mn></mrow>
                                    </msub>
                                </mrow>
                            </mfrac>
                        </mrow>
                    </mfenced>
                    <mfenced close=")" open="(" separators="">
                        <mrow>
                            <mfrac>
                                <mrow>
                                    <msub>
                                        <mrow><mi>a</mi></mrow><mrow><mn>1</mn></mrow>
                                    </msub>
