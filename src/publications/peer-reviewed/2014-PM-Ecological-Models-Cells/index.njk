        <!--l. 743-->
        <math display="inline" xmlns="http://www.w3.org/1998/Math/MathML">
            <mi>n</mi>
            <mrow>
                <mo class="MathClass-open">(</mo><mrow><mn>0</mn></mrow><mo class="MathClass-close">)</mo>
            </mrow>
            <mo class="MathClass-rel">=</mo> <mn>0</mn><mo class="MathClass-punc">.</mo><mn>0</mn><mn>1</mn>
