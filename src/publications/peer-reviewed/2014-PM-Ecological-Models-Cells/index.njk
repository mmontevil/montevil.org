                                </mrow>
                                <mrow>
                                    <msub>
                                        <mrow><mi>m</mi></mrow><mrow><mn>1</mn></mrow>
                                    </msub>
                                </mrow>
                            </mfrac>
                            <mo class="MathClass-bin">−</mo>
                            <msub>
                                <mrow><mi>q</mi></mrow><mrow><mn>2</mn><mn>1</mn></mrow>
                            </msub>
                            <mfrac>
                                <mrow>
                                    <msub>
                                        <mrow><mi>a</mi></mrow><mrow><mn>2</mn></mrow>
                                    </msub>
                                    <mi>φ</mi>
                                </mrow>
                                <mrow>
                                    <msub>
                                        <mrow><mi>m</mi></mrow><mrow><mn>2</mn></mrow>
                                    </msub>
                                </mrow>
                            </mfrac>
                        </mrow>
                    </mfenced>
                    <mspace width="2em"></mspace>
                </mtd>
                <mtd class="align-label" columnalign="right"></mtd> <mtd class="align-label"> <mspace width="2em"></mspace></mtd>
            </mtr>
            <mtr>
                <mtd class="align-odd" columnalign="right">
                    <msubsup>
                        <mrow><mi>N</mi></mrow><mrow><mn>2</mn></mrow><mrow><mo class="MathClass-bin">∗</mo></mrow>
                    </msubsup>
